Microhard-MIB DEFINITIONS ::= BEGIN

-- Title:	MICROHARD MIB
-- Date:	Sep 10, 2010
-- Disription: Not all parameters are supported in a paticular device,Support of parameters varies 
-- from version to version

IMPORTS
        MODULE-IDENTITY,
        OBJECT-IDENTITY,
        enterprises,
        OBJECT-TYPE, 
        NetworkAddress, 
        IpAddress,
        Counter, 
        Gauge, 
        TimeTicks
	FROM RFC1065-SMI;
--	DisplayString
--		FROM RFC1158-MIB;

Microhard OBJECT IDENTIFIER ::=	{ enterprises 21703 }

--VIPNNNN
	VIPNNNN				OBJECT IDENTIFIER ::=	{ Microhard 300 }
	Future				OBJECT IDENTIFIER ::=	{ Microhard 301 }
	SystemConfig			OBJECT IDENTIFIER ::=	{ VIPNNNN 1 }
	NetworkConfig			OBJECT IDENTIFIER ::=	{ VIPNNNN 2 }
	RadioConfig			OBJECT IDENTIFIER ::=	{ VIPNNNN 3 }
	COM1Config			OBJECT IDENTIFIER ::=	{ VIPNNNN 4 }
	COM2Config			OBJECT IDENTIFIER ::=	{ VIPNNNN 5 }
