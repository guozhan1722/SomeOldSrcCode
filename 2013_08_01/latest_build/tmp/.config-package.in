menuconfig IMAGEOPT
	bool "Image configuration"
	default n
	config UCI_PRECONFIG_network_lan_dns
		string "LAN DNS server" if IMAGEOPT
		depends PACKAGE_base-files
		default ""

	config UCI_PRECONFIG_network_lan_proto
		string "LAN Protocol" if IMAGEOPT
		depends PACKAGE_base-files
		default "static"

	config UCI_PRECONFIG_network_lan_gateway
		string "LAN Gateway" if IMAGEOPT
		depends PACKAGE_base-files
		default ""

	config UCI_PRECONFIG_network_lan_netmask
		string "LAN Network Mask" if IMAGEOPT
		depends PACKAGE_base-files
		default "255.255.255.0"

	config UCI_PRECONFIG_network_lan_ipaddr
		string "LAN IP Address" if IMAGEOPT
		depends PACKAGE_base-files
		default "192.168.1.1"

source "package/*/image-config.in"
